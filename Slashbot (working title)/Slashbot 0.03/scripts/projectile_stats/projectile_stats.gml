function projectile_stats(projectile){
	switch(projectile){
	case projectiles.miniball:
		proj_sprite = sp_miniball;
		firing_frames = 2;
		proj_frames = 1;
		hit_frames = 3;
		proj_speed = 8;
		damage = 1;
		persistance = 1;
	break;
	
	
	
	
	}
}